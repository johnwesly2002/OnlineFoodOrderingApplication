                </tr>
              </thead>
              <tbody>
            <tr ng-repeat="x in orderdata">
                <td>
                    <span>{{x.id}}</span>
                </td>
                <td>
                    <span>{{x.name}}</span>
                </td>
                <td>
                    <span>{{x.description}}</span>
                </td>
                <td>
                    <span>{{x.cost}}</span>
                </td>
                <td>
                    <span>{{x.item}}</span>
